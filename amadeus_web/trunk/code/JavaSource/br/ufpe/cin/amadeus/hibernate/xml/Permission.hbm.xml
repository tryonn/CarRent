<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping	PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="br.ufpe.cin.amadeus.system.human_resources.permission.Permission" table="permission">
 	<id name="id">
 		<column name="sqPermission" not-null="true" unique="true"/>
 		<generator class="increment"/>
 	</id>
 	<property name="name">
 		<column name="nmPermission" length="100" not-null="true" />
 	</property>
 	
 	<property name="permissionConstant">
 		<column name="idPermission" unique="true" />
 	</property>
 	
 
 </class>
</hibernate-mapping>