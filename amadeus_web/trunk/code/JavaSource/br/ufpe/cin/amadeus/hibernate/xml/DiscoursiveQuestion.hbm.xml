<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping	PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
       <class name="br.ufpe.cin.amadeus.system.academic.module.evaluation.question.DiscoursiveQuestion" table="discoursivequestion">
	        <id name="id">
	 			<column name="sqdiscoursivequestion" not-null="true" unique="true" />
	 			<generator class="increment"/>
	 		</id>
	
	 		<property name="question">
				<column name="ds_question"/>
			</property>
	
	
			<property name="level">
				<column name="id_level"/>
			</property>

			<property name="posInEvaluation">
				<column name="ds_posinevaluation" />
			</property>

       </class>


</hibernate-mapping>